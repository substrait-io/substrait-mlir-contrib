extension_uris {
  uri: "http://some.url/with/extensions.yml"
}
extensions {
  extension_function {
    name: "somefunc"
  }
}
relations {
  rel {
    aggregate {
      common {
        direct {
        }
      }
      input {
        read {
          common {
            direct {
            }
          }
          base_schema {
            names: "a"
            struct {
              types {
                i32 {
                  nullability: NULLABILITY_REQUIRED
                }
              }
              nullability: NULLABILITY_REQUIRED
            }
          }
          named_table {
            names: "t1"
          }
        }
      }
      groupings {
        grouping_expressions {
          literal {
            boolean: false
          }
        }
      }
      groupings {
        grouping_expressions {
          literal {
            boolean: false
          }
        }
        grouping_expressions {
          literal {
            boolean: true
          }
        }
      }
      groupings {
        grouping_expressions {
          literal {
            boolean: true
          }
        }
      }
      groupings {
      }
      measures {
        measure {
          phase: AGGREGATION_PHASE_INITIAL_TO_RESULT
          output_type {
            i32 {
              nullability: NULLABILITY_REQUIRED
            }
          }
          invocation: AGGREGATION_INVOCATION_ALL
          arguments {
            value {
              selection {
                direct_reference {
                  struct_field {
                  }
                }
                root_reference {
                }
              }
            }
          }
        }
      }
    }
  }
}
